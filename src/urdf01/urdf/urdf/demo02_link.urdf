<robot name="mycar">
    <link name="base_link">
    <!-- 可视化标签 -->
        <visual>
        <!-- 1.形状 -->
            <geometry>
                <!-- 1.1立方体 -->
                <box size="0.5 0.2 0.1" />
                <!-- 1.2 圆柱 -->
                <!-- <cylinder radius="0.1" length="0.2" /> -->
                <!-- 1.3 球体 -->
                <!-- <sphere radius="0.1" /> -->
                <!-- 1.4 皮肤  可导入stl模型-->
                <!-- <mesh filename="package://urdf01/meshes/mycar.stl"/> -->
            </geometry>
            <!-- 2.偏移量和倾斜弧度 -->
            <!-- 
                xyz设置偏移量m
                rpy设置绕xyz轴的旋转弧度 单位是弧度
             -->
            <origin xyz="0 0 0" rpy="0 0 0" />
            <!-- 3.颜色 -->
            <!-- 
            rgba 设置颜色和透明度
            r:红色 g:绿色 b:蓝色 a:透明度
            透明度、颜色范围0-1
            0表示完全透明 1表示完全不透明
             -->
            <material name="blue">
                <color rgba="0.8 0.8 1 1" />
            </material>
        </visual>
    </link>
</robot>