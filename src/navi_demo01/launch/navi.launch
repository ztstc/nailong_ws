<!-- 集成地图服务、amcl 、move_base -->
<launch>
    <!-- 设置地图的配置文件 -->
    <arg name="map" default="2d-1.yaml" />
    <!-- 运行地图服务器，并且加载设置的地图-->
    <node name="map_server" pkg="map_server" type="map_server" args="$(find navi_demo01)/map/$(arg map)"/>
    <!-- 启动AMCL节点 -->
    <include file="$(find navi_demo01)/launch/amcl.launch" />

    <!-- 运行move_base节点 -->
    <include file="$(find navi_demo01)/launch/movebase.launch" />

</launch>